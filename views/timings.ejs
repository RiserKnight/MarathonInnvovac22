<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0">
    <title>Trading - Parallel Computing Club(PCC)</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
   
</head>

<body>


    <section>
        <section class="main-content">
   
            <!-- Portfolio -->
            <div class="col-md-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4><b>Timings</b></h4>
                        <h3><b>Current: <%=curr%></b></h3>
                        <a href="/admin"><button>Admin Panel</button></a>
                    </div>
                    <div class="table-responsive">
                        <table class="table table-bordered table-hover">
                            <thead id="stockNameRow">
                                <tr>
                                    <th style="color: rgb(95, 98, 95);">
                                        <h4>timeID</h4>
                                    </th>
                                    <th style="color: rgb(95, 98, 95);">
                                        <h4>Purpose</h4>
                                    </th>
                                    <th style="color: rgb(95, 98, 95);">
                                        <h4>timeStamp</h4>
                                    </th>
                                    <th style="color: rgb(95, 98, 95);">
                                        <h4>Time</h4>
                                    </th>
                                </tr>
                            </thead>

                            <tbody>
                            <%timings.forEach((user)=>{%>
                                <tr>
                                    <td>
                                        <h5> <b><%=user.timeID%></b></h5>
                                    </td>
                                    <td>
                                        <h5><%=user.purpose%></h5>
                                    </td>
                                    <td>
                                        <h5><%=user.timeStamp%></h5>
                                    </td>
                                    <td>
                                        <%const d = new Date();%>
                                        <%d.setTime(user.timeStamp);%>
                                        
    <%const up=d.toLocaleString('en-US', {timeZone: "Asia/Kolkata"});%>
                                        <h5><%=up%></h5>
                                    </td>
                                
                                </tr>
                             <%})%>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            </div>
        </section>
    </section>

</body>
</html>